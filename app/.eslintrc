{
    "rules": {
        "indent": [
            2,
            4
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ]
    },
    "env": {
        "browser": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "no-alert": 1,
        "no-script-url": 1
    },
    "globals": {
        "angular": true,
        "$$config": true,
        "$": true,
        "google": true
    }
}